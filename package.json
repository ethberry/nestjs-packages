  "devDependencies": {
    "@gemunion/eslint-config": "0.1.9",
    "@typescript-eslint/eslint-plugin": "4.30.0",
    "@typescript-eslint/parser": "4.30.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-standard": "16.0.3",
    "eslint-import-resolver-ts": "0.4.2",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-jest": "24.4.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-promise": "5.1.0",
    "eslint-plugin-standard": "4.1.0",
    "husky": "4.3.8",
    "lint-staged": "11.1.2",
    "prettier": "2.3.2",
    "rimraf": "3.0.2",
    "typescript": "4.4.2"
  },
  "engines": {
    "node": ">=12.13"
  },
  "license": "UNLICENSED",
  "name": "@gemunion/common-packages",
  "repository": {
    "type": "git",
    "url": "https://github.com/gemunion/nestjs-packages"
  },
  "scripts": {
    "bootstrap": "lerna bootstrap --hoist",
    "build": "lerna run build --stream",
    "clean": "sh scripts/clean.sh",
    "kill": "sudo pkill -9 node",
    "lint": "lerna run lint --stream --parallel",
    "precommit": "lint-staged",
    "prettier": "prettier \"other/**/*.{ts,tsx}\" \"nest-js/**/*.{ts,tsx}\" \"nest-js-modules/**/*.{ts,tsx}\" \"material-ui/**/*.{ts,tsx}\" \"types/**/*.{ts,tsx}\" --write",
    "registry": "lerna publish",
    "release": "npm run release:version && npm run release:publish",
    "release:publish": "lerna publish from-package --yes --exact --ignore-scripts --registry=https://npm.pkg.github.com/",
    "release:version": "lerna version --yes --exact --conventional-commits",
    "storybook": "start-storybook -p 6006",
    "test": "sh scripts/test.sh"
  },
  "version": "0.0.1"
}
