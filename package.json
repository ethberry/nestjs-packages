    "@typescript-eslint/eslint-plugin": "5.18.0",
    "@typescript-eslint/parser": "5.18.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-standard": "16.0.3",
    "eslint-import-resolver-typescript": "2.7.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "26.1.4",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-promise": "6.0.0",
    "eslint-plugin-standard": "4.1.0",
    "husky": "4.3.8",
    "javascript-obfuscator": "4.0.0",
    "lint-staged": "12.3.7",
    "prettier": "2.6.2",
    "rimraf": "3.0.2",
    "typescript": "4.6.3"
  },
  "engines": {
    "node": ">=12.13"
  },
  "license": "UNLICENSED",
  "name": "@gemunion/common-packages",
  "repository": {
    "type": "git",
    "url": "https://github.com/gemunion/nestjs-packages"
  },
  "scripts": {
    "bootstrap": "lerna bootstrap --hoist",
    "build": "lerna run build --stream",
    "clean": "sh scripts/clean.sh",
    "kill": "sudo pkill -9 node",
    "lint": "lerna run lint --stream --parallel",
    "obfuscate": "sh scripts/obfuscate.sh",
    "precommit": "lint-staged",
    "prettier": "prettier \"modules/**/*.{ts,tsx}\" \"other/**/*.{ts,tsx}\" \"security/**/*.{ts,tsx}\" \"utils/**/*.{ts,tsx}\" --write",
    "registry": "lerna publish",
    "release": "npm run release:version && npm run release:publish",
    "release:publish": "lerna publish from-package --yes --exact --ignore-scripts --registry=https://npm.pkg.github.com/",
    "release:version": "lerna version",
    "renamedist": "sh scripts/renameo.sh",
    "storybook": "start-storybook -p 6006",
    "test": "sh scripts/test.sh"
  },
  "version": "0.0.1"
}
